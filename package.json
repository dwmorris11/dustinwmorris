{
  "name": "dustinwmorris",
  "version": "1.0.0",
  "description": "Chatbot UI",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:clienttypes && npm run build:server && npm run build:js",
    "build:server": "tsc -p ./clientserver/tsconfig.server.json",
    "build:clienttypes": "tsc -p src/tsconfig.client.json",
    "build:js": "webpack --config webpack.dev.js",
    "encrypt": "./node_modules/.bin/dotenvx encrypt -f clientserver/.env.local && ./node_modules/.bin/dotenvx encrypt -f clientserver/.env.production && ./node_modules/.bin/dotenvx encrypt -f .env",
    "start": "npm run build && npm run encrypt && dotenvx run -f clientserver/.env.local -- node ./server/server.js"
  },
  "author": "Dustin Morris",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.7.0",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fontsource/roboto": "latest",
    "@mui/base": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-static-gzip": "^2.1.7",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/material-ui": "latest",
    "@types/node": "^22.2.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
